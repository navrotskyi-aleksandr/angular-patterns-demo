{
    "env": {
        "name": "#{envName}"
    },
    "appInsights": {
        "instrumentationKey": "#{appInsightsKey}"
    },
    "logging": {
        "console": true,
        "appInsights": true,
        "traceEnabled": false
    },
    "aad": {
        "requireAuth": true,
        "tenant": "#{aadTenant}",
        "clientId": "#{aadClientId}",
        "endpoints": {
            "api": "#{aadClientId}"
        }
    },
    "apiServer": {
        "metadata": "#{medtadataApi}",
        "rules": "#{rulesApi}",
        "transaction": "#{transactionApi}"
    }
}
